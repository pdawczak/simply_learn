<h3>
  <%= @book.title %> <br/>
  <small><%= @book.isbn %></small>
</h3>

<p>
  <div class="thumbnail" style="float: left; margin-right: 20px; width: 142px;">
    <img src="<%= @book.image_url %>" alt="<%= @book.title %>">
  </div>
  <%= @book.description %>
</p>

<%= link "Back to list of books", to: book_path(@conn, :index) %>
